{% trans_default_domain 'FOSUserBundle' %}

<div class="fos_user_user_show">
    <dl>
        <dt>{{ 'profile.show.username'|trans }}</dt>
        <dd>{{ user.username }}</dd>
        <dt>{{ 'profile.show.email'|trans }}</dt>
        <dd>{{ user.email }}</dd>
        <dt>Name</dt>
        <dd>{{ user.name }}</dd>
        <dt>Age</dt>
        <dd>{{ user.age }}</dd>
        <dt>Job Title</dt>
        <dd>{{ user.jobTitle }}</dd>
    </dl>
</div>
